{
  "name": "react-dashboard-pi",
  "version": "1.0.0",
  "description": "a dashboard to use with a Raspberry PI and it's screen between 3.5 to 7",
  "main": "./src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack.config.dev.js",
    "build:production": "./node_modules/.bin/webpack --config webpack.config.production.js",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --content-base public/",
    "start-with-server": "./node_modules/.bin/webpack --config webpack.config.dev.js && python3 ../Server/server.py"
  },
  "homepage": "https://github.com/stormsa/dashboard_pi",
  "repository": "https://github.com/stormsa/dashboard_pi.git",
  "keywords": [
    "react",
    "webpack",
    "Raspberry PI",
    "react Node"
  ],
  "author": "Sofiane AZRI <sofiane.aziri@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.0.0",
    "popper.js": "^1.12.9",
    "react": "^16.2.1",
    "react-dom": "^16.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "bootstrap": "^4.0.0",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.11.0",
    "react-hot-loader": "^3.1.3",
    "react-responsive-carousel": "^3.1.33",
    "sass-loader": "^6.0.6",
    "url-loader": "^1.0.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-cli": "2.0.13",
    "webpack-config-utils": "2.0.0",
    "webpack-merge": "^3.0.0",
    "webpack-validator": "^3.0.1"
  }
}
